{
  "name": "jupyterlab-openbayes-theme",
  "version": "0.2.2",
  "description": "OpenBayes Theme",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://openbayes.com/",
  "bugs": {
    "url": "https://github.com/signcl/openbayes-jupyterlab-extensions/issues"
  },
  "license": "BSD-3-Clause",
  "author": "openbayes",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/signcl/openbayes-jupyterlab-extensions.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "watch-css": "sass --watch sass:style",
    "css": "sass sass:style"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "global": "^4.4.0",
    "sass": "^1.32.4"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typescript": "~3.7.3"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "themePath": "style/index.css"
  }
}
